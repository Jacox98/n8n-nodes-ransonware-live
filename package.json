{
  "name": "n8n-nodes-ransomware-live",
  "version": "0.1.0",
  "description": "Community nodes for interacting with the ransomware.live API.",
  "keywords": [
    "n8n-community-node-package",
    "ransomware",
    "threat-intelligence"
  ],
  "license": "MIT",
  "homepage": "https://www.ransomware.live",
  "author": {
    "name": "Jacox98",
    "email": "jacopo@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jacox98/n8n-nodes-ramsonware-live.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "icon": "dist/nodes/RansomwareLive/ransomwareLive.svg",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RansomwareLiveApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RansomwareLive/RansomwareLive.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "bugs": {
    "url": "https://github.com/Jacox98/n8n-nodes-ramsonware-live/issues"
  }
}
